{
  "build": {
    "beforeDevCommand": "npm run tauri-dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../webapp/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "diffedit3-gui",
    "version": "0.1.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false
      },
      "process": {
        "all": false,
        "exit": true,
        "relaunch": true
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Diffedit3",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": false,
      "identifier": "io.github.ilyagr.diffedit3"
    }
  }
}
